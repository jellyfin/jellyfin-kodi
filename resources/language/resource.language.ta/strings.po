msgstr "அதிகபட்ச கலைப்படைப்பு தீர்மானம்"

msgctxt "#33163"
msgid "Disabled/Media default"
msgstr "இயல்புநிலை மீடியா முடக்கப்பட்டது"

msgctxt "#30526"
msgid "Transcode AV1"
msgstr "டிரான்ஸ்கோட் AV1"

msgctxt "#30525"
msgid "Transcode VP9"
msgstr "டிரான்ஸ்கோட் VP9"

msgctxt "#33202"
