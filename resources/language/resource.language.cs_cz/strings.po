msgstr "Maximální počet zvukových kanálů"

msgctxt "#30163"
msgid "Audio bitrate"
msgstr "Datový tok audia"

msgctxt "#30162"
msgid "Preferred audio codec"
msgstr "Preferovaný audio kodek"

msgctxt "#30161"
msgid "Preferred video codec"
msgstr "Preferovaný video kodek"

msgctxt "#30160"
msgid "Max stream bitrate"
msgstr "Maximální datový tok"

msgctxt "#33163"
msgid "Disabled/Media default"
msgstr "Vypnuto/výchozí pro média"

msgctxt "#30526"
msgid "Transcode AV1"
msgstr "Překódovat AV1"

msgctxt "#30525"
